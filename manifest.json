{
  "manifest_version": 3,
  "name": "Common tabs",
  "version": "2.0",
  "description": "Easily open the usual suspects",
  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "action": {
    "default_title": "Common tabs",
    "default_icon": "icon_16.png",
    "default_popup": "window.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "michael@maty.us"
    }
  }
}
